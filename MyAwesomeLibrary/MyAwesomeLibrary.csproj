<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <PackageId>MahabaleshwarS.AwesomeLibrary</PackageId> <!-- Changed this line -->
    <Version>1.0.1</Version>
    <Authors>mahabaleshwars</Authors>
    <Company>mahabaleshwars</Company>
    <Description>A sample .NET library demonstrating string utilities and mathematical operations for NuGet packaging.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/mahabaleshwars/MyAwesomeLibrary</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageTags>utilities;string;math;sample</PackageTags>
    <PackageReleaseNotes>Initial release of MahabaleshwarS.AwesomeLibrary with string and math utilities.</PackageReleaseNotes>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

</Project>
